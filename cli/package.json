{
  "name": "agape-cli",
  "version": "0.0.1",
  "description": "Agape CLI",
  "main": "index.js",
  "scripts": {
    "start": "webpack --watch",
    "launch": "node dist/main.js",
    "build": "tsc && cp ./src/ag ./build && cp -r src/templates build",
    "pack": "rm *.tgz && npm run build && npm pack",
    "release": "npm run pack && npm publish --access public",
    "test": "jasmine-ts && exit 0"
  },
  "author": "Maverik Minett",
  "license": "ISC",
  "dependencies": {
    "@agape/object": "file:../ts/object/agape-object-1.0.2.tgz",
    "@agape/string": "file:../ts/string/agape-string-1.0.2.tgz",
    "@agape/templateer": "file:../ts/templateer/agape-templateer-1.0.3.tgz",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "commander": "^6.2.0",
    "configstore": "^5.0.1",
    "enquirer": "^2.3.6",
    "figlet": "^1.5.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "handlebars": "^4.7.6",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "touch": "^3.1.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.2",
    "@types/node": "^14.14.10",
    "copy-webpack-plugin": "^6.2.0",
    "jasmine": "^3.6.3",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "node-typescript": "^0.1.3",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "tsc": "^1.20150623.0",
    "typescript": "^4.1.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  },
  "browser": {
    "child_process": false
  }
}
